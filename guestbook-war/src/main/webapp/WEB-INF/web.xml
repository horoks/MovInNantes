<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>sign</servlet-name>
        <servlet-class>com.google.appengine.demos.guestbook.SignGuestbookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>guestbook</servlet-name>
        <servlet-class>com.google.appengine.demos.guestbook.GuestbookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ajax</servlet-name>
        <servlet-class>com.google.appengine.demos.guestbook.AjaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adresstan</servlet-name>
        <servlet-class>com.mov.in.nantes.tan.AdressTanservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParkingServlet</servlet-name>
        <servlet-class>com.move.in.nantes.cars.ParkingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveFavoritesServlet</servlet-name>
        <servlet-class>com.move.in.nantes.favorites.SaveFavoritesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RetrieveFavoritesServlet</servlet-name>
        <servlet-class>com.move.in.nantes.favorites.RetrieveFavoritesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParkingServlet</servlet-name>
        <url-pattern>/ParkingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adresstan</servlet-name>
        <url-pattern>/adresstan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sign</servlet-name>
        <url-pattern>/sign</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>guestbook</servlet-name>
        <url-pattern>/guestbook</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ajax</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveFavoritesServlet</servlet-name>
        <url-pattern>/SaveFavoritesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RetrieveFavoritesServlet</servlet-name>
        <url-pattern>/RetrieveFavoritesServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>guestbook.jsp</welcome-file>
    </welcome-file-list>
</web-app>
